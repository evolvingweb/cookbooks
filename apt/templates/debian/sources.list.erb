# Generated by Chef for <%= node[:fqdn] %>

<% node[:apt][:debian][:main_repos].each do |repo| %>
# <%= repo %>
deb <%= node[:apt][:debian][:mirror] %> <%= repo %> main contrib non-free 
deb-src <%= node[:apt][:debian][:mirror] %> <%= repo %> main contrib non-free 

<% end %>
<% node[:apt][:debian][:security_repos].each do |repo| %>
# <%= repo %> security
deb http://security.debian.org/ <%= repo %>/updates main 
deb-src http://security.debian.org/ <%= repo %>/updates main 

<% end %>
